{
    "name": "shell_async",
    "description": "Execute a shell command asynchronously",
    "command": "bash -c",
    "timeout_seconds": 30,
    "force_synchronous": false,
    "enabled": true,
    "subcommand": [
        {
            "name": "default",
            "description": "Execute command",
            "positional_args": [
                {
                    "name": "command",
                    "type": "string",
                    "description": "The command to execute",
                    "required": true
                }
            ]
        }
    ]
}
